{
    "name": "atc/whx4",
    "type": "wordpress-plugin",
    "description": "A WordPress plugin for managing People, Places, and Events (Who/What/Where/When). Also serves as a core plugin with functionality shared by SDG, MLib, and others.",
  	"keywords": ["wordpress", "plugin", "people", "places", "events"],
    "authors": [
        {
            "name": "atc",
            "email": "birdhive@gmail.com",
            "homepage": "https://birdhive.com/"
        }
    ],
	"require": {
		"php": ">=7.0",
        "composer/installers": "^1.10"
	},
    "autoload": {
        "psr-4": {
            "atc\\WHx4\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
